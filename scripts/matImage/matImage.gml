function matImage(i){
	
	if(i == Mat.wood){ return imgPupWood; }
	if(i == Mat.gel){ return imgPupGel; }
	if(i == Mat.gold){ return imgPupGold; }
	if(i == Mat.mythril){ return imgPupMythril; }
	if(i == Mat.adamantite){ return imgPupAdamantite; }
	if(i == Mat.core){ return imgPupCore; }
	
	
	
	return imgBlank;
}